{
  "name": "metaed-plugin-edfi-sql-dictionary",
  "main": "dist/index.js",
  "version": "1.2.1-dev.4",
  "description": "Ed-Fi Alliance SQL Data Dictionary plugin for MetaEd",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "registry": "https://www.myget.org/F/ed-fi/npm/"
  },
  "metaed-plugin": {
    "artifact-specific": {
      "shortName": "edfiSqlDictionary",
      "authorName": "Ed-Fi Alliance",
      "metaEdVersion": "1.x",
      "technologyVersion": "2.x",
      "dependencies": [
        "metaed-plugin-edfi-unified",
        "metaed-plugin-edfi-xsd"
      ]
    }
  },
  "dependencies": {
    "handlebars": "^4.0.10",
    "metaed-core": "1.2.1-dev.4",
    "metaed-plugin-edfi-ods": "1.2.1-dev.4",
    "metaed-plugin-edfi-unified": "1.2.1-dev.4",
    "winston": "3.0.0",
    "xlsx": "0.11.19"
  },
  "scripts": {
    "build": "yarn build:clean && yarn build:dist && yarn build:flow",
    "build:clean": "rimraf dist",
    "build:flow": "flow-copy-source -v src dist",
    "build:dist": "rimraf dist && babel src -d dist --source-maps inline --copy-files"
  }
}
